{
  "name": "starp",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "scripts": {
    "start": "yarn build && node ./dist/main/index.js",
    "build": "yarn tsc --build",
    "dev": "ts-node-dev -r tsconfig-paths/register --transpile-only --ignore-watch node_modules,coverage,dist src/main/index.ts",
    "prepare": "husky install",
    "test:unit": "yarn cross-env NODE_ENV=test jest --config ./jest.config.js --passWithNoTests",
    "test:integration": "yarn cross-env NODE_ENV=test jest --config ./tests/jest.config.js --passWithNoTests --runInBand"
  },
  "devDependencies": {
    "@types/jest": "^28.1.0",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "cross-env": "^7.0.3",
    "eslint": ">=5.16.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^26.4.6",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.1",
    "jest": "^28.1.0",
    "jest-mock-extended": "^2.0.6",
    "lint-staged": "^13.0.0",
    "pino-pretty": "^8.0.0",
    "prettier": "^2.6.2",
    "prisma": "^3.14.0",
    "ts-jest": "^28.0.4",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.3"
  },
  "prisma": {
    "schema": "./src/shared/infrastructure/database/schema.prisma"
  },
  "dependencies": {
    "@prisma/client": "^3.14.0",
    "dotenv": "^16.0.1",
    "winston": "^3.7.2"
  }
}
